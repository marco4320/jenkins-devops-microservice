// declarative pipeline
pipeline {
	agent any
	stage('Build') {
		steps {
			echo "Build"
		}
	}
	stage('Test') {
		steps {
			echo "Test"
		}
	}
	stage('Integration Test') {
		steps {
			echo "Integration Test"
		}
	}
}
